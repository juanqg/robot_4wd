<launch>
    <!-- differential_teleop_key already has its own built in velocity
    smoother -->
    <node pkg="robot_model_pkg" type="diff_wheeled_robot_key" name="diff_wheeled_robot_key" output="screen" launch-prefix="xterm -e">
        <param name="scale_linear" value="0.5" type="double"/>
        <param name="scale_angular" value="1.5" type="double"/>
        <remap from="/cmd_vel" to="/robot_model_pkg/robot_model_pkg_controller/cmd_vel"/>
    </node>
</launch>
